{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "allowJs": true,
    "checkJs": false,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "strict": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "#relay": [
        "src/index.mjs"
      ],
      "#relay/relay": [
        "src/relay/index.mjs"
      ],
      "#relay/relay/initiate": [
        "src/relay/initiate/index.mjs"
      ],
      "#relay/relay/regulate": [
        "src/relay/regulate/index.mjs"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}
