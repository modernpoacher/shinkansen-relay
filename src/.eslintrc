{
  "overrides": [
    {
      "files": [
        "*.mts",
        "*.cts"
      ],
      "plugins": [
        "@typescript-eslint"
      ],
      "extends": "standard-with-typescript",
      "parser": "@typescript-eslint/parser",
      "rules": {
        "@typescript-eslint/no-extraneous-class": "off"
      }
    }
  ]
}
